<p id="notice"><%= notice %></p>

<h1><%= link_to 'Сотрудники ',workers_path %> Должности</h1>


<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @positions.each do |position| %>
      <tr>
        <td><%= position.title %></td>
        <td><%= link_to 'Show', position %></td>
        <td><%= link_to 'Edit', edit_position_path(position) %></td>
        <td><%= link_to 'Destroy', position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @positions, :previous_label => t("Назад"), :next_label => t("Вперёд")  %>

<br>

<%= link_to 'New Position', new_position_path %>
